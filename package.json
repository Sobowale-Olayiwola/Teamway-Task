{
    "name": "node-template",
    "version": "3.0.0",
    "description": "Boilerplate for our node applications. This templates serves as a guide for teams synchronization.",
    "main": "index.js",
    "author": "Oguntuberu Nathan O.",
    "license": "ISC",
    "homepage": "https://github.com/noguntuberu/node-template#readme",
    "bugs": {
        "url": "https://github.com/noguntuberu/node-template/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noguntuberu/node-template.git"
    },
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-mongo-sanitize": "^2.2.0",
        "glob": "^7.1.6",
        "handlebars": "^4.7.7",
        "helmet": "^5.0.2",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^8.5.1",
        "mockdate": "^3.0.5",
        "mongoose": "^6.2.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "node-cron": "^3.0.0",
        "nodemailer": "^6.7.0",
        "redis": "^3.1.2",
        "run-script-os": "^1.1.6",
        "sequelize": "^6.15.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/glob": "^7.2.0",
        "@types/hpp": "^0.2.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/morgan": "^1.9.3",
        "@types/node-cron": "^3.0.0",
        "@types/nodemailer": "^6.4.4",
        "@types/redis": "^2.8.32",
        "chai": "^4.3.6",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-security": "^1.4.0",
        "jsdoc": "^3.6.7",
        "mocha": "^9.2.1",
        "nodemon": "^2.0.4",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "proxyquire": "^2.1.3",
        "sinon": "^13.0.1"
    },
    "scripts": {
        "dev": "nodemon -r dotenv/config index.js",
        "test": "nyc mocha --recursive -w -c || true",
        "test-publish": "nyc --reporter=html mocha --recursive -w -c || true",
        "test:browser": "live-server --port=9000 --mount=/:public",
        "test:coverage": "nyc --reporter=html --check-coverage=true mocha --recursive -c || true",
        "update-example-env": "run-script-os",
        "update-example-env:darwin:linux": "cp .env .env.example",
        "update-example-env:win32": "robocopy .env .env.example",
        "update-documentation": "jsdoc -c jsdoc.json",
        "generate-env": "run-script-os",
        "generate-env:darwin:linux": "cp .env.example .env",
        "generate-env:win32": "robocopy .env.example .env",
        "start": "node -r dotenv/config index.js"
    }
}
